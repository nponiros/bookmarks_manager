    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-mui-speeddial": "0.0.5",
    "react-redux": "4.4.6",
    "react-tap-event-plugin": "2.0.1",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0"
  },
  "devDependencies": {
    "babel-core": "6.20.0",
    "babel-loader": "6.2.9",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "css-loader": "0.26.1",
    "eslint": "3.12.0",
    "eslint-config-airbnb": "13.0.0",
